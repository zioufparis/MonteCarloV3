{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["dom", "es2017"],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true
  }
}
